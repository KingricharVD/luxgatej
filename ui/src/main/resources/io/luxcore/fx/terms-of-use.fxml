<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" stylesheets="@../../../modena_dark.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.luxcore.fx.TermsOfUseController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <bottom>
                <BorderPane prefHeight="50.0" prefWidth="580.0" BorderPane.alignment="CENTER">
                    <right>
                        <Button fx:id="next" mnemonicParsing="false" text="Next" BorderPane.alignment="CENTER" onAction="#termsAgreed">
                            <BorderPane.margin>
                                <Insets right="10.0" />
                            </BorderPane.margin>
                        </Button>
                    </right>
                    <left>
                        <CheckBox fx:id="agree" mnemonicParsing="false" text="I agree" BorderPane.alignment="CENTER">
                            <BorderPane.margin>
                                <Insets left="10.0" />
                            </BorderPane.margin>
                        </CheckBox>
                    </left>
                </BorderPane>
            </bottom>
         <center>
            <WebView fx:id="webView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
        </BorderPane>
    </children>
</AnchorPane>
